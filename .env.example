# Electrs Server Configuration
# Copy this file to .env and fill in your actual values

# Electrs server hostname or IP address
ELECTRS_HOST=your.umbrel.node.ip

# Electrs TCP port (default: 50001)
ELECTRS_PORT=50001

# Whether to use SSL/TLS encryption (true/false)
ELECTRS_USE_SSL=false

# Electrs SSL port (default: 50002, only used if ELECTRS_USE_SSL=true)
ELECTRS_SSL_PORT=50002